{% extends 'base.html.twig' %}

{% block body %}

    <div class="col-10 offset-1 my-5 col-xl-6 offset-xl-3">
        <div class="text-center my-5 border-bottom">
            <h1 class="display-4 text-uppercase">Updated real estate</h1>
        </div>
        {{ form_start(form) }}
        {{ include('properties/_form.html.twig', {form: form}) }}
        <button type="submit" class="col-md-8 offset-md-2 container text-center btn btn-primary rounded-0 my-4 text-uppercase">Update
        </button>
        {{ form_end(form, { render_rest: false }) }}
    </div>


{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('addAsset') }}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('addAsset') }}
{% endblock %}

{% block title %}

{% endblock %}

